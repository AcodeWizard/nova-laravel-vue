{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-age-range": "cd nova-components/AgeRange && npm run dev",
        "build-age-range-prod": "cd nova-components/AgeRange && npm run prod",
        "build-input-range": "cd nova-components/InputRange && npm run dev",
        "build-input-range-prod": "cd nova-components/InputRange && npm run prod",
        "build-newinput-range": "cd nova-components/NewinputRange && npm run dev",
        "build-newinput-range-prod": "cd nova-components/NewinputRange && npm run prod",
        "build-check-range": "cd nova-components/CheckRange && npm run dev",
        "build-check-range-prod": "cd nova-components/CheckRange && npm run prod",
        "build-last-filter": "cd nova-components/LastFilter && npm run dev",
        "build-last-filter-prod": "cd nova-components/LastFilter && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.21",
        "bootstrap": "^4.0.0",
        "cross-env": "^5.1",
        "jquery": "^3.2",
        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.5",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0",
        "vue": "^2.5.17",
        "vue-slider-component": "^2.8.16",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "nodemon": "^1.18.11"
    }
}